head
  base(href="/")
  meta(charset='utf-8')
  meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
  meta(name='viewport', content='width=device-width,initial-scale=1')

  title= appName+' - '+title
  meta(http-equiv='Content-type', content='text/html;charset=UTF-8')
  meta(name="keywords", content="node.js, express, mongoose, mongodb, angularjs")
  meta(name="description", content="Anerve - Shopping With Friends")
  link(href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css',rel='stylesheet' , type='text/css')
  link(href='/public/assets/img/icons/favicon.ico', rel='shortcut icon', type='image/x-icon')
  //link(href='/public/assets/style.css',rel='stylesheet' typeHtml='jade' , type='text/css')
  link(href='/public/assets/html5imageupload/html5imageupload.css',rel='stylesheet' , type='text/css')

  
  meta(property='fb:app_id', content='APP_ID')
  meta(property='og:title', content='#{appName} - #{title}')
  meta(property='og:description', content='Anerve - Shopping With Friends')
  meta(property='og:type', content='website')
  meta(property='og:url', content='APP_URL')
  meta(property='og:image', content='APP_LOGO')
  meta(property='og:site_name', content='Anerve - Shopping With Friends')
  meta(property='fb:admins', content='APP_ADMIN')


  if(process.env.NODE_ENV == 'production')   
    link(rel='stylesheet', href='/bundle/bundle.min.css')
    each bowerCSSAsset in settings.assetsapp.core.css['bower_components/build/css/dist.min.css']
      link(rel='stylesheet', href='/#{bowerCSSAsset}')
  else
    each bowerCSSAsset in settings.assetsapp.core.css['bower_components/build/css/dist.min.css']
      link(rel='stylesheet', href='/#{bowerCSSAsset}')
    each cssAsset in settings.cssAssets
      link(rel='stylesheet', href='#{cssAsset}')
  
  script(src='https://code.jquery.com/jquery-1.7.2.js')
  script(src='https://code.jquery.com/ui/1.12.1/jquery-ui.js')
  script(src='https://checkout.stripe.com/checkout.js')
  script(src='https://js.stripe.com/v2/')
  script(src='https://js.stripe.com/v3/')
 script(src='https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.0.4/socket.io.js')
  


  //if lt IE 9
    script(src='http://html5shim.googlecode.com/svn/trunk/html5.js')
    
  
  
